      "form_factor": "wide",
      "label": "D-PlaneOS Dashboard"
    },
    {
      "src": "/assets/screenshots/mobile.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "D-PlaneOS Mobile View"
    }
  ],
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Home",
      "description": "Open D-PlaneOS dashboard",
      "url": "/",
      "icons": [
        {
          "src": "/assets/icons/shortcut-home.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Storage Pools",
      "short_name": "Pools",
      "description": "Manage ZFS storage pools",
      "url": "/pages/pools.html",
      "icons": [
        {
          "src": "/assets/icons/shortcut-storage.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "File Browser",
      "short_name": "Files",
      "description": "Browse and manage files",
      "url": "/pages/files.html",
      "icons": [
        {
          "src": "/assets/icons/shortcut-files.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Docker Containers",
      "short_name": "Docker",
      "description": "Manage Docker containers",
      "url": "/pages/docker.html",
      "icons": [
        {
          "src": "/assets/icons/shortcut-docker.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/api/files/upload",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": ["*/*"]
        }
      ]
    }
  },
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "protocol_handlers": [
    {
      "protocol": "web+dplaneos",
      "url": "/?action=%s"
    }
  ]
}
